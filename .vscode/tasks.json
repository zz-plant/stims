{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "bun:dev",
      "type": "shell",
      "command": "bun run dev",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "fileLocation": "absolute",
        "pattern": {
          "regexp": "^(.*)$",
          "message": 1
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "vite v",
          "endsPattern": "ready in"
        }
      }
    },
    {
      "label": "bun:test",
      "type": "shell",
      "command": "bun test",
      "problemMatcher": []
    }
  ]
}
