{
  "name": "stims",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "npm run jest --",
    "jest": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint",
    "format": "prettier --write .",
    "dev": "vite",
    "build": "vite build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/three": "^0.181.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^8.55.0",
    "husky": "^9.0.11",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.2",
    "vite": "^6.3.5"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "jsdom",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "engines": {
    "node": "^22"
  },
  "dependencies": {
    "three": "^0.177.0"
  }
}
